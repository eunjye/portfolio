@charset "UTF-8";
.line-top, .line-bottom {
  position: absolute;
  width: 0;
  height: 5px;
  background: black; }

.line-right, .line-left {
  position: absolute;
  width: 5px;
  height: 0;
  background: black; }

.link-wrap.new-pf::after, .link-wrap.old-pf::after {
  position: absolute;
  display: block;
  content: "";
  top: 0;
  left: 0;
  width: 100%;
  height: 100%; }

